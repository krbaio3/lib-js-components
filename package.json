{
  "name": "@css/ui-kit",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "static-files/index.cjs.js",
  "module": "static-files/index.js",
  "types": "static-files/types/index.d.ts",
  "collection": "static-files/collection/collection-manifest.json",
  "collection:main": "static-files/collection/index.js",
  "unpkg": "static-files/lib-js-components/lib-js-components.esm.js",
  "exports": {
    ".": {
      "import": "./static-files/lib-js-components/lib-js-components.esm.js",
      "require": "./static-files/lib-js-components/lib-js-components.cjs.js"
    },
    "./my-component": {
      "import": "./static-files/components/my-component.js",
      "types": "./static-files/components/my-component.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/stencil-component-starter.git"
  },
  "files": [
    "static-files/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "watch": "stencil build --watch",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "lint": "eslint src/**/*{.ts,.tsx}",
    "generate": "stencil generate",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "docker:build": "docker build -t @css/ui-kit-local-cdn:0.0.1 .",
    "docker:run": "docker run -p 5000:80 @css/ui-kit-local-cdn:0.0.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "1.6.1",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.9.1",
    "@stencil-community/eslint-plugin": "^0.8.0",
    "@stencil/core": "4.20.0",
    "@stencil/sass": "^3.0.12",
    "@storybook/addon-essentials": "^8.2.8",
    "@storybook/addon-interactions": "^8.2.8",
    "@storybook/addon-links": "^8.2.8",
    "@storybook/addon-mdx-gfm": "^8.2.8",
    "@storybook/blocks": "^8.2.8",
    "@storybook/html": "^8.2.8",
    "@storybook/html-vite": "^8.2.8",
    "@storybook/test": "^8.2.8",
    "@storybook/web-components-vite": "^8.2.8",
    "@types/jest": "29.5.12",
    "@types/node": "22.1.0",
    "eslint": "^9.9.1",
    "globals": "^15.9.0",
    "jest": "29.7.0",
    "jest-cli": "29.7.0",
    "lit": "^3.2.0",
    "puppeteer": "23.0.1",
    "storybook": "^8.2.8",
    "storybook-addon-stencil": "^0.2.1",
    "typescript-eslint": "^8.3.0"
  },
  "license": "MIT",
  "dependencies": {
    "@storybook/web-components": "^8.2.8",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "prettier": "^3.3.3",
    "sass": "^1.77.8",
    "typescript": "^5.5.4",
    "vite": "5.3.5",
    "vite-plugin-sass-dts": "^1.3.25"
  }
}
